<?xml version="1.0"?>
<bidi:Definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:bidi="http://schemas.microsoft.com/windows/2005/03/printing/bidi"
                 xsi:schemaLocation="http://schemas.microsoft.com/windows/2005/03/printing/bidi wsdbidi.xsd">
  <Schema xmlns:nprt="http://schemas.microsoft.com/windows/2006/08/wdp/print"
          xmlns:hpprt="http://www.hp.com/schemas/imaging/con/hpprt/2014/08/22"
          xmlns:wprt="http://schemas.microsoft.com/windows/2006/08/wdp/print">
    <Property name="Printer">
      <Property name="PaperSource">
          <Property name="Source1">
            <Installed name="InputInstalled" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main-roll"]' drvPrinterEvent="true"/>
			<Value name="Name" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main-roll"]/wprt:InputBinEntry' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="Type" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main-roll"]/hpprt:Type' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="NotReady" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main-roll"]/hpprt:isReady' type="BIDI_BOOL" drvPrinterEvent="true"/>
			<Value name="Width" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main-roll"]/hpprt:MediaWidth' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Length" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main-roll"]/hpprt:MediaHeight' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Id" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main-roll"]/hpprt:MediaID' type="BIDI_STRING" drvPrinterEvent="true"/>
          </Property>
        <Property name="Source2">
            <Installed name="InputInstalled" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main"]' drvPrinterEvent="true"/>
			<Value name="Name" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main"]/wprt:InputBinEntry' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="Type" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main"]/hpprt:Type' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="NotReady" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main"]/hpprt:isReady' type="BIDI_BOOL" drvPrinterEvent="true"/>
			<Value name="Width" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main"]/hpprt:MediaWidth' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Length" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main"]/hpprt:MediaHeight' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Id" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="main"]/hpprt:MediaID' type="BIDI_STRING" drvPrinterEvent="true"/>
          </Property>
		<Property name="Source3">
            <Installed name="InputInstalled" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="manual"]' drvPrinterEvent="true"/>
			<Value name="Name" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="manual"]/wprt:InputBinEntry' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="Type" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="manual"]/hpprt:Type' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="NotReady" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="manual"]/hpprt:isReady' type="BIDI_BOOL" drvPrinterEvent="true"/>
			<Value name="Width" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="manual"]/hpprt:MediaWidth' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Length" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="manual"]/hpprt:MediaHeight' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Id" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="manual"]/hpprt:MediaID' type="BIDI_STRING" drvPrinterEvent="true"/>
          </Property>    			  
      </Property>
    </Property>
  </Schema>
</bidi:Definition>