<?xml version="1.0"?>
<bidi:Definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:bidi="http://schemas.microsoft.com/windows/2005/03/printing/bidi"
                 xsi:schemaLocation="http://schemas.microsoft.com/windows/2005/03/printing/bidi wsdbidi.xsd">
  <Schema xmlns:nprt="http://schemas.microsoft.com/windows/2006/08/wdp/print"
          xmlns:hpprt="http://www.hp.com/schemas/imaging/con/hpprt/2014/08/22"
          xmlns:wprt="http://schemas.microsoft.com/windows/2006/08/wdp/print">
    <Property name="Printer">
      <Property name="PaperSource">
          <Property name="Source1">
            <Installed name="InputInstalled" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="MainRollTray"]' drvPrinterEvent="true"/>
			<Value name="Name" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="MainRollTray"]/wprt:InputBinEntry' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="Type" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="MainRollTray"]/hpprt:Type' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="NotReady" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="MainRollTray"]/hpprt:IsReady' type="BIDI_BOOL" drvPrinterEvent="true"/>
			<Value name="Width" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="MainRollTray"]/hpprt:Width' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Length" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="MainRollTray"]/hpprt:Length' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Id" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="MainRollTray"]/hpprt:MediaId' type="BIDI_STRING" drvPrinterEvent="true"/>
          </Property>
        <Property name="Source2">
            <Installed name="InputInstalled" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Top"]' drvPrinterEvent="true"/>
			<Value name="Name" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Top"]/wprt:InputBinEntry' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="Type" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Top"]/hpprt:Type' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="NotReady" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Top"]/hpprt:IsReady' type="BIDI_BOOL" drvPrinterEvent="true"/>
			<Value name="Width" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Top"]/hpprt:Width' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Length" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Top"]/hpprt:Length' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Id" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Top"]/hpprt:MediaId' type="BIDI_STRING" drvPrinterEvent="true"/>
          </Property>
		<Property name="Source3">
            <Installed name="InputInstalled" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Tray1"]' drvPrinterEvent="true"/>
			<Value name="Name" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Tray1"]/wprt:InputBinEntry' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="Type" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Tray1"]/hpprt:Type' type="BIDI_STRING" drvPrinterEvent="true"/>
			<Value name="NotReady" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Tray1"]/hpprt:IsReady' type="BIDI_BOOL" drvPrinterEvent="true"/>
			<Value name="Width" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Tray1"]/hpprt:Width' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Length" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Tray1"]/hpprt:Length' type="BIDI_INT" drvPrinterEvent="true"/>
			<Value name="Id" query="wprt:PrinterConfiguration" filter='wprt:PrinterConfiguration/wprt:InputBins/wprt:InputBinEntry[@wprt:Name="Tray1"]/hpprt:MediaId' type="BIDI_STRING" drvPrinterEvent="true"/>
          </Property>    			  
      </Property>
    </Property>
  </Schema>
</bidi:Definition>